<form
    role="form"
    id="create-hometab-form"
    class="form-horizontal"
    action="claro_admin_home_tab_create"
    method="post"
    {{ form_enctype(form) }}
>
    <div class="panel-body">
        {{ form_widget(form) }}
    </div>
    <div class="form-actions panel-footer">
        <button id="form-create-hometab-ok-btn" type="submit" class="btn btn-primary">
            {{ 'ok'|trans({}, 'platform') }}
        </button>
        <button id="form-cancel-btn" type="button" class="btn btn-default">
            {{ 'cancel'|trans({}, 'platform') }}
        </button>
    </div>
</form>
