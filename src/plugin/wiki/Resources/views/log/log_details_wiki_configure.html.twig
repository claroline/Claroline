{% if log.details.wiki is defined %}
    {% if log.getDetails().wiki.changeSet is defined and log.getDetails().wiki.changeSet|length > 0 %}
    <div>
        <h4>{{ ('log_change_set')|trans({}, 'log') }}</h4>
        <h5>{{ ('log_old_values')|trans({}, 'log') }}</h5>
        {% set changeSet = log.getDetails().wiki.changeSet %}
        <dl class="dl-horizontal">
            {% for key, values in changeSet %}
                <dt>{{ key|trans({}, 'log') }}</dt>
                {% if values[0] is null or values[0] is sameas(0)%}
                    <dd>{{ "normal"|trans({}, 'log') }}</dd>
                {% elseif values[0] is sameas(1) %}
                    <dd>{{ "moderate"|trans({}, 'actions') }}</dd>
                {% elseif values[0] is sameas(2) %}
                    <dd>{{ "read_only"|trans({}, 'actions') }}</dd>
                {% else %}
                    <dd>{{ values[0]|raw }}</dd>
                {% endif %}
            {% endfor %}
        </dl>
        <h5>{{ ('log_new_values')|trans({}, 'log') }}</h5>
        <dl class="dl-horizontal">
            {% for key, values in changeSet %}
                <dt>{{ key|trans({}, 'log') }}</dt>
                {% if values[1] is null or values[1] is sameas(0)%}
                    <dd>{{ "normal"|trans({}, 'log') }}</dd>
                {% elseif values[1] is sameas(1) %}
                    <dd>{{ "moderate"|trans({}, 'log') }}</dd>
                {% elseif values[1] is sameas(2) %}
                    <dd>{{ "read_only"|trans({}, 'log') }}</dd>
                {% else %}
                    <dd>{{ values[1]|raw }}</dd>
                {% endif %}
            {% endfor %}
        </dl>
    </div>
    {% endif %}
{% endif %}
